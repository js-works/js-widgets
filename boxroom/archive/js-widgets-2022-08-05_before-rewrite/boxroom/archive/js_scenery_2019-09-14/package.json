{
  "name": "js-scenery",
  "version": "0.0.14",
  "license": "LGPL-3.0",
  "homepage": "https://github.com/js-works/js-scenery",
  "repository": {
    "type": "git",
    "url": "https://github.com/js-works/js-scenery.git"
  },
  "main": "index.js",
  "module": "dist/js-scenery.all.esm.production.js",
  "unpkg": "dist/js-scenery.all.umd.production.js",
  "jsdelivr": "dist/js-scenery.all.umd.production.js",
  "types": "dist/types/modules/all/main/index.d.ts",
  "files": [
    "index.js",
    "core.js",
    "html.js",
    "svg.js",
    "dist"
  ],
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.3",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "bestzip": "^2.1.4",
    "chai": "^4.2.0",
    "cli-watch": "^1.0.7",
    "copyfiles": "^2.1.1",
    "coveralls": "^3.0.6",
    "cross-conf-env": "^1.1.2",
    "cross-env": "^5.2.0",
    "dyo": "^1.0.0",
    "js-spec": "^0.1.67",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-gzip": "^2.2.0",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-tslint": "^0.2.2",
    "rollup-plugin-typescript2": "^0.24.0",
    "rollup-plugin-uglify": "^6.0.3",
    "rollup-plugin-uglify-es": "0.0.1",
    "shx": "^0.3.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2",
    "ziptool": "^1.0.2"
  },
  "scripts": {
    "clean": "shx rm -rf ./build ./dist ./coverage",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=4096 rollup --c rollup.config.js && tsc -d --emitDeclarationOnly --declarationDir dist/types",
    "test": "nyc mocha -r ts-node/register -r source-map-support/register src/modules/**/test/**/*.ts",
    "test-watch": "mocha -w -r ts-node/register -r source-map-support/register src/modules/**/test/**/*.ts",
    "demo": "rollup --c rollup.demo.config.js -w",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "zipsource": "shx rm -rf ./build/source && shx mkdir -p ./build/source && copyfiles -a ./* ./build/source && shx cp -r ./src ./build/source && shx mkdir -p ./dist/source && cd build/source && bestzip ../../dist/source/source.zip . && cd ../..",
    "dist": "npm run clean && npm run test && npm run build && npm run zipsource"
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "nyc": {
    "temp-directory": "./coverage/.nyc_output",
    "include": [
      "src/modules"
    ],
    "exclude": [],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "all": true
  }
}
